{
  "name": "@acme/react-package-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@acme/style-guide/prettier",
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@acme/style-guide": "workspace:*",
    "@acme/vite": "workspace:*",
    "@tsconfig/vite-react": "^2.0.1",
    "@types/react": "^18.2.73",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.5.3"
  }
}
